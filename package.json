{
  "name": "family-logistics-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "format": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "setup:supabase": "node scripts/verify-supabase-setup.js",
    "supabase:verify": "node scripts/verify-supabase-setup.js",
    "supabase:init": "bash scripts/setup-supabase.sh"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss,md,json,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "chart.js": "^4.4.7",
    "pinia": "^2.1.7",
    "supabase": "^2.76.12",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^9.14.3",
    "vue-router": "^4.3.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^8.0.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "patch-package": "^8.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "ajv": "^6.14.0"
  }
}
